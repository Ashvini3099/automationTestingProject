package Ashu;

public class Overloading {
	public static void main(String[] args) {
		Overloading  obj = new Overloading();
		obj.bike();
		obj.bike(120000,"Black");
		obj.bike("Audi");
		obj.main(20);
		obj.main(30,40);
		
	}
	// you can also overload main method 
	public static void main(int p) {
		System.out.println("main method executed");
		System.out.println(p);
		
	}
	public static void main(int q,int h) {
		System.out.println("main method executed");
		System.out.println(q+h);
		
	}
	public void bike() {
		System.out.println("Car is very expensive");
//		System.out.println();
	}
	public void bike(int price,String  color) {
		System.out.println("Car is very expensive");
		System.out.println(price);
		System.out.println(color);
		}
	public void bike(String model) {
		System.out.println("car is very expensive");
		System.out.println(model);
		
	}
	
}